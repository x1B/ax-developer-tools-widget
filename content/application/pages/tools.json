{
   "layout": "tools",
   "areas": {
      "activities": [
         {
            "widget": "developer-tools/ax-host-connector-widget",
            "features": {
               "events": {
                  "stream": "eventBusItems"
               },
               "log": {
                  "stream": "logItems"
               },
               "pageInfo": {
                  "resource": "pageInfo"
               },
               "grid": {
                  "resource": "gridSettings"
               }
            }
         }
      ],
      "toolbar": [
         {
            "widget": "developer-tools/ax-developer-toolbar-widget",
            "id": "toolbar",
            "features": {
               "grid": {
                  "resource": "gridSettings"
               },
               "tabs": {
                  "parameter": "tab"
               }
            }
         }
      ],
      "toolbar.page": [
         {
            "widget": "developer-tools/ax-page-inspector-widget",
            "features": {
               "pageInfo": {
                  "resource": "pageInfo"
               },
               "filter": {
                  "resource": "eventFilter"
               }
            }
         }
      ],
      "toolbar.events": [
         {
            "widget": "developer-tools/ax-events-display-widget",
            "features": {
               "events": {
                  "stream": "eventBusItems"
               },
               "filter": {
                  "resource": "eventFilter"
               }
            }
         }
      ],
      "toolbar.logs": [
         {
            "widget": "developer-tools/ax-log-display-widget",
            "features": {
               "log": {
                  "stream": "logItems"
               }
            }
         }
      ]
   }
}
