            "includes/widgets/developer-tools/ax-page-inspector-widget/default.theme/css/ax-page-inspector-widget.css",
            "includes/widgets/developer-tools/ax-page-inspector-widget/ax-page-inspector-widget.js"
         ],
         "event": [
