            "includes/widgets/developer-tools/ax-host-connector-widget/*",
            "includes/widgets/developer-tools/ax-host-connector-widget/default.theme/*",
            "includes/widgets/developer-tools/ax-host-connector-widget/default.theme/css/*",
            "includes/widgets/developer-tools/ax-log-display-widget/*",
            "includes/widgets/developer-tools/ax-log-display-widget/default.theme/*",
            "includes/widgets/developer-tools/ax-log-display-widget/default.theme/css/*",
            "includes/widgets/developer-tools/ax-page-inspector-widget/*",
            "includes/widgets/developer-tools/ax-page-inspector-widget/default.theme/*",
            "includes/widgets/developer-tools/ax-page-inspector-widget/default.theme/css/*"
         ],
         "event": [
            "changed"
         ],
         "tasks": [
            "laxar-resources:main"
         ],
         "options": {
            "event": [
               "added",
               "deleted"
            ],
            "spawn": false
         }
      },
      "main-rebuild": {
         "files": [
            "application/flow/flow.json",
            "application/pages/tools.json"
         ],
         "tasks": [
            "laxar-build:main"
         ],
         "options": {
            "spawn": false
         }
      },
      "main-merge-require-config": {
         "files": [
            "includes/widgets/developer-tools/ax-developer-toolbar-widget/require_config.js",
            "includes/widgets/developer-tools/ax-events-display-widget/require_config.js",
            "includes/widgets/developer-tools/ax-host-connector-widget/require_config.js",
            "includes/widgets/developer-tools/ax-log-display-widget/require_config.js",
            "includes/widgets/developer-tools/ax-page-inspector-widget/require_config.js",
            "require_config.js"
         ],
         "event": [
            "changed"
         ],
         "tasks": [
            "laxar-merge-require-config:main"
         ],
         "options": {
            "spawn": false
         }
      }
   }
