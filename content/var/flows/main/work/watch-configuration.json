      },
      "main-update": {
         "files": [
            "application/flow/*",
            "application/pages/*",
            "application/layouts/tools/default.theme/css/*",
            "application/layouts/tools/default.theme/*",
            "includes/widgets/developer-tools/ax-developer-toolbar-widget/*",
